<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.store.repository.OrderMapper">
    <select id="selectAllOrders" resultType="com.example.store.model.Order">
        SELECT o.id as "id" FROM `order` o;
    </select>
</mapper>
